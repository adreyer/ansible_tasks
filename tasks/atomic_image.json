{
  "input_method": "stdin", 
  "description": "Manage the container images on the atomic host platform", 
  "parameters": {
    "started": {
      "type": "Optional[Enum[\"yes\", \"no\"]]", 
      "description": "Start or Stop the container"
    }, 
    "state": {
      "type": "Optional[Enum[\"present\", \"absent\", \"latest\"]]", 
      "description": "The state of the container image."
    }, 
    "name": {
      "type": "String[0]", 
      "description": "Name of the container image"
    }, 
    "backend": {
      "type": "Optional[Enum[\"docker\", \"ostree\"]]", 
      "description": "Define the backend where the image is pulled."
    }
  }
}