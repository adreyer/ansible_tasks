{
  "input_method": "stdin", 
  "description": "Create, update, delete pods.", 
  "parameters": {
    "api_key": {
      "type": "Optional[String[0]]", 
      "description": "API key of the CloudStack API."
    }, 
    "api_url": {
      "type": "Optional[String[0]]", 
      "description": "URL of the CloudStack API e.g. https://cloud.example.com/client/api."
    }, 
    "api_http_method": {
      "type": "Optional[Enum[\"get\", \"post\"]]", 
      "description": "HTTP method used."
    }, 
    "api_secret": {
      "type": "Optional[String[0]]", 
      "description": "Secret key of the CloudStack API."
    }, 
    "api_timeout": {
      "type": "Optional[String[0]]", 
      "description": "HTTP timeout."
    }, 
    "api_region": {
      "type": "Optional[String[0]]", 
      "description": "Name of the ini section in the C(cloustack.ini) file."
    }, 
    "gateway": {
      "type": "Optional[String[0]]", 
      "description": "Gateway for the Pod."
    }, 
    "state": {
      "type": "Optional[Enum[\"present\", \"enabled\", \"disabled\", \"absent\"]]", 
      "description": "State of the pod."
    }, 
    "end_ip": {
      "type": "Optional[String[0]]", 
      "description": "Ending IP address for the Pod."
    }, 
    "start_ip": {
      "type": "Optional[String[0]]", 
      "description": "Starting IP address for the Pod."
    }, 
    "zone": {
      "type": "Optional[String[0]]", 
      "description": "Name of the zone in which the pod belongs to."
    }, 
    "netmask": {
      "type": "Optional[String[0]]", 
      "description": "Netmask for the Pod."
    }, 
    "id": {
      "type": "Optional[String[0]]", 
      "description": "uuid of the exising pod."
    }, 
    "name": {
      "type": "String[0]", 
      "description": "Name of the pod."
    }
  }
}