{
  "input_method": "stdin", 
  "description": "Create, update, delete user roles.", 
  "parameters": {
    "name": {
      "type": "String[0]", 
      "description": "Name of the role."
    }, 
    "api_http_method": {
      "type": "Optional[Enum[\"get\", \"post\"]]", 
      "description": "HTTP method used."
    }, 
    "api_secret": {
      "type": "Optional[String[0]]", 
      "description": "Secret key of the CloudStack API."
    }, 
    "description": {
      "type": "Optional[String[0]]", 
      "description": "Description of the role."
    }, 
    "api_timeout": {
      "type": "Optional[String[0]]", 
      "description": "HTTP timeout."
    }, 
    "api_region": {
      "type": "Optional[String[0]]", 
      "description": "Name of the ini section in the C(cloustack.ini) file."
    }, 
    "state": {
      "type": "Optional[Enum[\"present\", \"absent\"]]", 
      "description": "State of the role."
    }, 
    "api_key": {
      "type": "Optional[String[0]]", 
      "description": "API key of the CloudStack API."
    }, 
    "id": {
      "type": "Optional[String[0]]", 
      "description": "ID of the role."
    }, 
    "role_type": {
      "type": "Optional[Enum[\"User\", \"DomainAdmin\", \"ResourceAdmin\", \"Admin\"]]", 
      "description": "Type of the role."
    }, 
    "api_url": {
      "type": "Optional[String[0]]", 
      "description": "URL of the CloudStack API e.g. https://cloud.example.com/client/api."
    }
  }
}