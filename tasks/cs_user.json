{
  "input_method": "stdin", 
  "description": "Create, update, disable, lock, enable and remove users.", 
  "parameters": {
    "username": {
      "type": "String[0]", 
      "description": "Username of the user."
    }, 
    "domain": {
      "type": "Optional[String[0]]", 
      "description": "Domain the user is related to."
    }, 
    "last_name": {
      "type": "Optional[String[0]]", 
      "description": "Last name of the user."
    }, 
    "poll_async": {
      "type": "Optional[String[0]]", 
      "description": "Poll async jobs until job has finished."
    }, 
    "api_url": {
      "type": "Optional[String[0]]", 
      "description": "URL of the CloudStack API e.g. https://cloud.example.com/client/api."
    }, 
    "api_http_method": {
      "type": "Optional[Enum[\"get\", \"post\"]]", 
      "description": "HTTP method used."
    }, 
    "api_secret": {
      "type": "Optional[String[0]]", 
      "description": "Secret key of the CloudStack API."
    }, 
    "api_timeout": {
      "type": "Optional[String[0]]", 
      "description": "HTTP timeout."
    }, 
    "first_name": {
      "type": "Optional[String[0]]", 
      "description": "First name of the user."
    }, 
    "api_region": {
      "type": "Optional[String[0]]", 
      "description": "Name of the ini section in the C(cloustack.ini) file."
    }, 
    "api_key": {
      "type": "Optional[String[0]]", 
      "description": "API key of the CloudStack API."
    }, 
    "state": {
      "type": "Optional[Enum[\"present\", \"absent\", \"enabled\", \"disabled\", \"locked\", \"unlocked\"]]", 
      "description": "State of the user."
    }, 
    "account": {
      "type": "Optional[String[0]]", 
      "description": "Account the user will be created under."
    }, 
    "timezone": {
      "type": "Optional[String[0]]", 
      "description": "Timezone of the user."
    }, 
    "password": {
      "type": "Optional[String[0]]", 
      "description": "Password of the user to be created."
    }, 
    "email": {
      "type": "Optional[String[0]]", 
      "description": "Email of the user."
    }
  }
}