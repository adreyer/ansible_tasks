{
  "input_method": "stdin", 
  "description": "Create, update and delete VPCs.", 
  "parameters": {
    "display_text": {
      "type": "Optional[String[0]]", 
      "description": "Display text of the VPC."
    }, 
    "domain": {
      "type": "Optional[String[0]]", 
      "description": "Domain the VPC is related to."
    }, 
    "vpc_offering": {
      "type": "Optional[String[0]]", 
      "description": "Name of the VPC offering."
    }, 
    "poll_async": {
      "type": "Optional[String[0]]", 
      "description": "Poll async jobs until job has finished."
    }, 
    "name": {
      "type": "String[0]", 
      "description": "Name of the VPC."
    }, 
    "api_http_method": {
      "type": "Optional[Enum[\"get\", \"post\"]]", 
      "description": "HTTP method used."
    }, 
    "tags": {
      "type": "Optional[String[0]]", 
      "description": "List of tags. Tags are a list of dictionaries having keys C(key) and C(value)."
    }, 
    "api_secret": {
      "type": "Optional[String[0]]", 
      "description": "Secret key of the CloudStack API."
    }, 
    "api_timeout": {
      "type": "Optional[String[0]]", 
      "description": "HTTP timeout."
    }, 
    "account": {
      "type": "Optional[String[0]]", 
      "description": "Account the VPC is related to."
    }, 
    "api_region": {
      "type": "Optional[String[0]]", 
      "description": "Name of the ini section in the C(cloustack.ini) file."
    }, 
    "project": {
      "type": "Optional[String[0]]", 
      "description": "Name of the project the VPC is related to."
    }, 
    "api_key": {
      "type": "Optional[String[0]]", 
      "description": "API key of the CloudStack API."
    }, 
    "zone": {
      "type": "Optional[String[0]]", 
      "description": "Name of the zone."
    }, 
    "state": {
      "type": "Optional[Enum[\"present\", \"absent\", \"restarted\"]]", 
      "description": "State of the VPC."
    }, 
    "cidr": {
      "type": "Optional[String[0]]", 
      "description": "CIDR of the VPC, e.g. 10.1.0.0/16"
    }, 
    "network_domain": {
      "type": "Optional[String[0]]", 
      "description": "Network domain for the VPC."
    }, 
    "api_url": {
      "type": "Optional[String[0]]", 
      "description": "URL of the CloudStack API e.g. https://cloud.example.com/client/api."
    }
  }
}