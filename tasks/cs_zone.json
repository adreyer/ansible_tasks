{
  "input_method": "stdin", 
  "description": "Create, update and remove zones.", 
  "parameters": {
    "internal_dns1": {
      "type": "Optional[String[0]]", 
      "description": "First internal DNS for the zone."
    }, 
    "domain": {
      "type": "Optional[String[0]]", 
      "description": "Domain the zone is related to."
    }, 
    "api_key": {
      "type": "Optional[String[0]]", 
      "description": "API key of the CloudStack API."
    }, 
    "name": {
      "type": "String[0]", 
      "description": "Name of the zone."
    }, 
    "api_http_method": {
      "type": "Optional[Enum[\"get\", \"post\"]]", 
      "description": "HTTP method used."
    }, 
    "state": {
      "type": "Optional[Enum[\"present\", \"enabled\", \"disabled\", \"absent\"]]", 
      "description": "State of the zone."
    }, 
    "api_secret": {
      "type": "Optional[String[0]]", 
      "description": "Secret key of the CloudStack API."
    }, 
    "dhcp_provider": {
      "type": "Optional[String[0]]", 
      "description": "DHCP provider for the Zone."
    }, 
    "api_timeout": {
      "type": "Optional[String[0]]", 
      "description": "HTTP timeout."
    }, 
    "dns1": {
      "type": "Optional[String[0]]", 
      "description": "First DNS for the zone."
    }, 
    "dns2_ipv6": {
      "type": "Optional[String[0]]", 
      "description": "Second DNS for IPv6 for the zone."
    }, 
    "api_region": {
      "type": "Optional[String[0]]", 
      "description": "Name of the ini section in the C(cloustack.ini) file."
    }, 
    "internal_dns2": {
      "type": "Optional[String[0]]", 
      "description": "Second internal DNS for the zone."
    }, 
    "guest_cidr_address": {
      "type": "Optional[String[0]]", 
      "description": "Guest CIDR address for the zone."
    }, 
    "dns2": {
      "type": "Optional[String[0]]", 
      "description": "Second DNS for the zone."
    }, 
    "api_url": {
      "type": "Optional[String[0]]", 
      "description": "URL of the CloudStack API e.g. https://cloud.example.com/client/api."
    }, 
    "network_domain": {
      "type": "Optional[String[0]]", 
      "description": "Network domain for the zone."
    }, 
    "id": {
      "type": "Optional[String[0]]", 
      "description": "uuid of the exising zone."
    }, 
    "network_type": {
      "type": "Optional[Enum[\"basic\", \"advanced\"]]", 
      "description": "Network type of the zone."
    }, 
    "dns1_ipv6": {
      "type": "Optional[String[0]]", 
      "description": "First DNS for IPv6 for the zone."
    }
  }
}