{
  "input_method": "stdin", 
  "description": "Allows to post events to DataDog (www.datadoghq.com) service.", 
  "parameters": {
    "date_happened": {
      "type": "Optional[String[0]]", 
      "description": "POSIX timestamp of the event."
    }, 
    "alert_type": {
      "type": "Optional[Enum[\"error\", \"warning\", \"info\", \"success\"]]", 
      "description": "Type of alert."
    }, 
    "tags": {
      "type": "Optional[String[0]]", 
      "description": "Comma separated list of tags to apply to the event."
    }, 
    "text": {
      "type": "String[0]", 
      "description": "The body of the event."
    }, 
    "title": {
      "type": "String[0]", 
      "description": "The event title."
    }, 
    "app_key": {
      "type": "String[0]", 
      "description": "Your DataDog app key."
    }, 
    "priority": {
      "type": "Optional[Enum[\"normal\", \"low\"]]", 
      "description": "The priority of the event."
    }, 
    "aggregation_key": {
      "type": "Optional[String[0]]", 
      "description": "An arbitrary string to use for aggregation."
    }, 
    "api_key": {
      "type": "String[0]", 
      "description": "Your DataDog API key."
    }, 
    "validate_certs": {
      "type": "Optional[Enum[\"yes\", \"no\"]]", 
      "description": "If C(no), SSL certificates will not be validated. This should only be used on personally controlled sites using self-signed certificates."
    }
  }
}