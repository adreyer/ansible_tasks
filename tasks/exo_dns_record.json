{
  "input_method": "stdin", 
  "description": "Create, update and delete records.", 
  "parameters": {
    "domain": {
      "type": "String[0]", 
      "description": "Domain the record is related to."
    }, 
    "multiple": {
      "type": "Optional[String[0]]", 
      "description": "Whether there are more than one records with similar C(name)."
    }, 
    "name": {
      "type": "Optional[String[0]]", 
      "description": "Name of the record."
    }, 
    "prio": {
      "type": "Optional[String[0]]", 
      "description": "Priority of the record."
    }, 
    "api_secret": {
      "type": "Optional[String[0]]", 
      "description": "Secret key of the Exoscale DNS API."
    }, 
    "api_timeout": {
      "type": "Optional[String[0]]", 
      "description": "HTTP timeout to Exoscale DNS API."
    }, 
    "api_region": {
      "type": "Optional[String[0]]", 
      "description": "Name of the ini section in the C(cloustack.ini) file."
    }, 
    "content": {
      "type": "Optional[String[0]]", 
      "description": "Content of the record."
    }, 
    "record_type": {
      "type": "Optional[Enum[\"A\", \"ALIAS\", \"CNAME\", \"MX\", \"SPF\", \"URL\", \"TXT\", \"NS\", \"SRV\", \"NAPTR\", \"PTR\", \"AAAA\", \"SSHFP\", \"HINFO\", \"POOL\"]]", 
      "description": "Type of the record."
    }, 
    "state": {
      "type": "Optional[Enum[\"present\", \"absent\"]]", 
      "description": "State of the record."
    }, 
    "ttl": {
      "type": "Optional[String[0]]", 
      "description": "TTL of the record in seconds."
    }, 
    "api_key": {
      "type": "Optional[String[0]]", 
      "description": "API key of the Exoscale DNS API."
    }, 
    "validate_certs": {
      "type": "Optional[String[0]]", 
      "description": "Validate SSL certs of the Exoscale DNS API."
    }
  }
}