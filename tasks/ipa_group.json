{
  "input_method": "stdin", 
  "description": "Add, modify and delete group within IPA server", 
  "parameters": {
    "ipa_port": {
      "type": "Optional[String[0]]", 
      "description": "P"
    }, 
    "group": {
      "type": "Optional[String[0]]", 
      "description": "List of group names assigned to this group."
    }, 
    "cn": {
      "type": "String[0]", 
      "description": "Canonical name."
    }, 
    "user": {
      "type": "Optional[String[0]]", 
      "description": "List of user names assigned to this group."
    }, 
    "ipa_prot": {
      "type": "Optional[Enum[\"http\", \"https\"]]", 
      "description": "P"
    }, 
    "state": {
      "type": "Optional[Enum[\"present\", \"absent\"]]", 
      "description": "State to ensure"
    }, 
    "gidnumber": {
      "type": "Optional[String[0]]", 
      "description": "GID (use this option to set it manually)."
    }, 
    "external": {
      "type": "Optional[String[0]]", 
      "description": "Allow adding external non-IPA members from trusted domains."
    }, 
    "ipa_user": {
      "type": "Optional[String[0]]", 
      "description": "A"
    }, 
    "ipa_pass": {
      "type": "String[0]", 
      "description": "P"
    }, 
    "validate_certs": {
      "type": "Optional[String[0]]", 
      "description": "This only applies if C(ipa_prot) is I(https)."
    }, 
    "nonposix": {
      "type": "Optional[String[0]]", 
      "description": "Create as a non-POSIX group."
    }, 
    "ipa_host": {
      "type": "Optional[String[0]]", 
      "description": "I"
    }
  }
}