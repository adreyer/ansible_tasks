{
  "input_method": "stdin", 
  "description": "Add, modify and delete an IPA host using IPA API", 
  "parameters": {
    "ipa_port": {
      "type": "Optional[String[0]]", 
      "description": "P"
    }, 
    "force": {
      "type": "Optional[String[0]]", 
      "description": "Force host name even if not in DNS."
    }, 
    "description": {
      "type": "Optional[String[0]]", 
      "description": "A description of this host."
    }, 
    "state": {
      "type": "Optional[Enum[\"present\", \"absent\", \"disabled\"]]", 
      "description": "S"
    }, 
    "ns_host_location": {
      "type": "Optional[String[0]]", 
      "description": "Host location (e.g. \"Lab 2\")"
    }, 
    "mac_address": {
      "type": "Optional[String[0]]", 
      "description": "List of Hardware MAC address(es) off this host."
    }, 
    "ipa_prot": {
      "type": "Optional[Enum[\"http\", \"https\"]]", 
      "description": "P"
    }, 
    "fqdn": {
      "type": "String[0]", 
      "description": "Full qualified domain name."
    }, 
    "validate_certs": {
      "type": "Optional[String[0]]", 
      "description": "This only applies if C(ipa_prot) is I(https)."
    }, 
    "ns_os_version": {
      "type": "Optional[String[0]]", 
      "description": "Host operating system and version (e.g. \"Fedora 9\")"
    }, 
    "ipa_host": {
      "type": "Optional[String[0]]", 
      "description": "I"
    }, 
    "ipa_user": {
      "type": "Optional[String[0]]", 
      "description": "A"
    }, 
    "user_certificate": {
      "type": "Optional[String[0]]", 
      "description": "List of Base-64 encoded server certificates."
    }, 
    "ipa_pass": {
      "type": "String[0]", 
      "description": "P"
    }, 
    "ip_address": {
      "type": "Optional[String[0]]", 
      "description": "Add the host to DNS with this IP address."
    }, 
    "ns_hardware_platform": {
      "type": "Optional[String[0]]", 
      "description": "Host hardware platform (e.g. \"Lenovo T61\")"
    }
  }
}