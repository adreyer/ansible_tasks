{
  "input_method": "stdin", 
  "description": "Add, modify and delete an IPA host-group using IPA API", 
  "parameters": {
    "ipa_port": {
      "type": "Optional[String[0]]", 
      "description": "P"
    }, 
    "cn": {
      "type": "String[0]", 
      "description": "Name of host-group."
    }, 
    "ipa_prot": {
      "type": "Optional[Enum[\"http\", \"https\"]]", 
      "description": "P"
    }, 
    "state": {
      "type": "Optional[Enum[\"present\", \"absent\"]]", 
      "description": "State to ensure."
    }, 
    "hostgroup": {
      "type": "Optional[String[0]]", 
      "description": "List of host-groups than belong to that host-group."
    }, 
    "host": {
      "type": "Optional[String[0]]", 
      "description": "List of hosts that belong to the host-group."
    }, 
    "ipa_host": {
      "type": "Optional[String[0]]", 
      "description": "I"
    }, 
    "ipa_user": {
      "type": "Optional[String[0]]", 
      "description": "A"
    }, 
    "ipa_pass": {
      "type": "String[0]", 
      "description": "P"
    }, 
    "validate_certs": {
      "type": "Optional[String[0]]", 
      "description": "This only applies if C(ipa_prot) is I(https)."
    }, 
    "description": {
      "type": "Optional[String[0]]", 
      "description": "Description"
    }
  }
}