{
  "input_method": "stdin", 
  "description": "Add, modify and delete a role within FreeIPA server using FreeIPA API", 
  "parameters": {
    "ipa_port": {
      "type": "Optional[String[0]]", 
      "description": "P"
    }, 
    "group": {
      "type": "Optional[String[0]]", 
      "description": "List of group names assign to this role."
    }, 
    "cn": {
      "type": "String[0]", 
      "description": "Role name."
    }, 
    "service": {
      "type": "Optional[String[0]]", 
      "description": "List of service names to assign."
    }, 
    "user": {
      "type": "Optional[String[0]]", 
      "description": "List of user names to assign."
    }, 
    "ipa_prot": {
      "type": "Optional[Enum[\"http\", \"https\"]]", 
      "description": "P"
    }, 
    "state": {
      "type": "Optional[Enum[\"present\", \"absent\"]]", 
      "description": "S"
    }, 
    "hostgroup": {
      "type": "Optional[String[0]]", 
      "description": "List of host group names to assign."
    }, 
    "host": {
      "type": "Optional[String[0]]", 
      "description": "List of host names to assign."
    }, 
    "ipa_host": {
      "type": "Optional[String[0]]", 
      "description": "I"
    }, 
    "ipa_user": {
      "type": "Optional[String[0]]", 
      "description": "A"
    }, 
    "ipa_pass": {
      "type": "String[0]", 
      "description": "P"
    }, 
    "validate_certs": {
      "type": "Optional[String[0]]", 
      "description": "This only applies if C(ipa_prot) is I(https)."
    }, 
    "description": {
      "type": "Optional[String[0]]", 
      "description": "A description of this role-group."
    }
  }
}