{
  "input_method": "stdin", 
  "description": "Add, modify or delete sudo command within FreeIPA server using FreeIPA API.", 
  "parameters": {
    "ipa_port": {
      "type": "Optional[String[0]]", 
      "description": "P"
    }, 
    "description": {
      "type": "Optional[String[0]]", 
      "description": "A description of this command."
    }, 
    "ipa_prot": {
      "type": "Optional[Enum[\"http\", \"https\"]]", 
      "description": "P"
    }, 
    "state": {
      "type": "Optional[Enum[\"present\", \"absent\"]]", 
      "description": "S"
    }, 
    "ipa_host": {
      "type": "Optional[String[0]]", 
      "description": "I"
    }, 
    "ipa_user": {
      "type": "Optional[String[0]]", 
      "description": "A"
    }, 
    "ipa_pass": {
      "type": "String[0]", 
      "description": "P"
    }, 
    "sudocmd": {
      "type": "String[0]", 
      "description": "Sudo Command."
    }, 
    "validate_certs": {
      "type": "Optional[String[0]]", 
      "description": "This only applies if C(ipa_prot) is I(https)."
    }
  }
}