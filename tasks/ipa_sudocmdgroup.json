    }, 
    "ipa_prot": {
      "type": "Optional[Enum[\"http\", \"https\"]]", 
      "description": "P"
    }, 
    "state": {
      "type": "Optional[Enum[\"present\", \"absent\"]]", 
      "description": "S"
    }, 
    "ipa_host": {
      "type": "Optional[String[0]]", 
      "description": "I"
    }, 
    "ipa_user": {
      "type": "Optional[String[0]]", 
      "description": "A"
    }, 
    "ipa_pass": {
      "type": "String[0]", 
      "description": "P"
    }, 
    "sudocmd": {
      "type": "Optional[String[0]]", 
