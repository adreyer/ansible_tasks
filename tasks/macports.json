{
  "input_method": "stdin", 
  "description": "Manages MacPorts packages", 
  "parameters": {
    "state": {
      "type": "Optional[Enum[\"present\", \"absent\", \"active\", \"inactive\"]]", 
      "description": "state of the package"
    }, 
    "update_cache": {
      "type": "Optional[Enum[\"yes\", \"no\"]]", 
      "description": "update the package db first"
    }, 
    "name": {
      "type": "String[0]", 
      "description": "name of package to install/remove"
    }
  }
}