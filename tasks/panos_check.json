{
  "input_method": "stdin", 
  "description": "Check if PAN-OS device is ready for being configured (no pending jobs).", 
  "parameters": {
    "username": {
      "type": "Optional[String[0]]", 
      "description": "username for authentication"
    }, 
    "password": {
      "type": "String[0]", 
      "description": "password for authentication"
    }, 
    "ip_address": {
      "type": "String[0]", 
      "description": "IP address (or hostname) of PAN-OS device"
    }, 
    "timeout": {
      "type": "Optional[String[0]]", 
      "description": "timeout of API calls"
    }, 
    "interval": {
      "type": "Optional[String[0]]", 
      "description": "time waited between checks"
    }
  }
}