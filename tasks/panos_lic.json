{
  "input_method": "stdin", 
  "description": "Apply an authcode to a device.", 
  "parameters": {
    "username": {
      "type": "Optional[String[0]]", 
      "description": "username for authentication"
    }, 
    "force": {
      "type": "Optional[String[0]]", 
      "description": "whether to apply authcode even if device is already licensed"
    }, 
    "password": {
      "type": "String[0]", 
      "description": "password for authentication"
    }, 
    "ip_address": {
      "type": "String[0]", 
      "description": "IP address (or hostname) of PAN-OS device"
    }, 
    "auth_code": {
      "type": "String[0]", 
      "description": "authcode to be applied"
    }
  }
}