{
  "input_method": "stdin", 
  "description": "Configure management settings of device", 
  "parameters": {
    "username": {
      "type": "Optional[String[0]]", 
      "description": "username for authentication"
    }, 
    "panorama_primary": {
      "type": "Optional[String[0]]", 
      "description": "address of primary Panorama server"
    }, 
    "dns_server_secondary": {
      "type": "Optional[String[0]]", 
      "description": "address of secondary DNS server"
    }, 
    "dns_server_primary": {
      "type": "Optional[String[0]]", 
      "description": "address of primary DNS server"
    }, 
    "panorama_secondary": {
      "type": "Optional[String[0]]", 
      "description": "address of secondary Panorama server"
    }, 
    "commit": {
      "type": "Optional[String[0]]", 
      "description": "commit if changed"
    }, 
    "password": {
      "type": "String[0]", 
      "description": "password for authentication"
    }, 
    "ip_address": {
      "type": "String[0]", 
      "description": "IP address (or hostname) of PAN-OS device"
    }
  }
}