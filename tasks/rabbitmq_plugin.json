{
  "input_method": "stdin", 
  "description": "Enables or disables RabbitMQ plugins", 
  "parameters": {
    "state": {
      "type": "Optional[Enum[\"enabled\", \"disabled\"]]", 
      "description": "Specify if plugins are to be enabled or disabled"
    }, 
    "names": {
      "type": "String[0]", 
      "description": "Comma-separated list of plugin names"
    }, 
    "prefix": {
      "type": "Optional[String[0]]", 
      "description": "Specify a custom install prefix to a Rabbit"
    }, 
    "new_only": {
      "type": "Optional[Enum[\"yes\", \"no\"]]", 
      "description": "Only enable missing plugins"
    }
  }
}