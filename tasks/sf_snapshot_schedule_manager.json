{
  "input_method": "stdin", 
  "description": "Create, destroy, or update accounts on SolidFire", 
  "parameters": {
    "username": {
      "type": "String[0]", 
      "description": "Please ensure that the user has the adequate permissions. For more information, please read the official documentation U(https://goo.gl/ddJa4Q)."
    }, 
    "time_interval_days": {
      "type": "Optional[String[0]]", 
      "description": "T"
    }, 
    "name": {
      "type": "String[0]", 
      "description": "Name for the snapshot schedule."
    }, 
    "time_interval_minutes": {
      "type": "Optional[String[0]]", 
      "description": "T"
    }, 
    "hostname": {
      "type": "String[0]", 
      "description": "The hostname or IP address of the SolidFire cluster."
    }, 
    "state": {
      "type": "Enum[\"present\", \"absent\"]", 
      "description": "Whether the specified schedule should exist or not."
    }, 
    "time_interval_hours": {
      "type": "Optional[String[0]]", 
      "description": "T"
    }, 
    "paused": {
      "type": "Optional[String[0]]", 
      "description": "Pause / Resume a schedule."
    }, 
    "schedule_id": {
      "type": "Optional[String[0]]", 
      "description": "The schedule ID for the schedule that you want to update or delete."
    }, 
    "snapshot_name": {
      "type": "Optional[String[0]]", 
      "description": "Name for the created snapshots."
    }, 
    "volumes": {
      "type": "Optional[String[0]]", 
      "description": "Volume IDs that you want to set the snapshot schedule for."
    }, 
    "starting_date": {
      "type": "Optional[String[0]]", 
      "description": "Starting date for the schedule."
    }, 
    "password": {
      "type": "String[0]", 
      "description": "Password for the specified user."
    }, 
    "recurring": {
      "type": "Optional[String[0]]", 
      "description": "Should the schedule recur?"
    }, 
    "retention": {
      "type": "Optional[String[0]]", 
      "description": "Retention period for the snapshot."
    }
  }
}