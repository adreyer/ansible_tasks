{
  "input_method": "stdin", 
  "description": "Create, update, or destroy Ansible Tower users. See U(https://www.ansible.com/tower) for an overview.", 
  "parameters": {
    "username": {
      "type": "String[0]", 
      "description": "The username of the user."
    }, 
    "superuser": {
      "type": "Optional[String[0]]", 
      "description": "User is a system wide administator."
    }, 
    "first_name": {
      "type": "Optional[String[0]]", 
      "description": "First name of the user."
    }, 
    "last_name": {
      "type": "Optional[String[0]]", 
      "description": "Last name of the user."
    }, 
    "tower_host": {
      "type": "Optional[String[0]]", 
      "description": "URL to your Tower instance."
    }, 
    "tower_config_file": {
      "type": "Optional[String[0]]", 
      "description": "Path to the Tower config file. See notes."
    }, 
    "state": {
      "type": "Optional[Enum[\"present\", \"absent\"]]", 
      "description": "Desired state of the resource."
    }, 
    "tower_verify_ssl": {
      "type": "Optional[String[0]]", 
      "description": "Dis/allow insecure connections to Tower. If C(no), SSL certificates will not be validated. This should only be used on personally controlled sites using self-signed certificates."
    }, 
    "auditor": {
      "type": "Optional[String[0]]", 
      "description": "User is a system wide auditor."
    }, 
    "tower_password": {
      "type": "Optional[String[0]]", 
      "description": "Password for your Tower instance."
    }, 
    "tower_username": {
      "type": "Optional[String[0]]", 
      "description": "Username for your Tower instance."
    }, 
    "password": {
      "type": "Optional[String[0]]", 
      "description": "Password of the user."
    }, 
    "email": {
      "type": "String[0]", 
      "description": "Email address of the user."
    }
  }
}